<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_common" name="Shared Front-end and Back-end assets" inherit_id="web.assets_common">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/jowebutils/static/src/js/define.js" />
        </xpath>
    </template>

    <template id="assets_frontend" name="Junari Web Utils Assets Frontend" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <link type="text/css" rel="stylesheet" href="/jowebutils/static/src/css/forms.css"/>
            <link type="text/css" rel="stylesheet" href="/jowebutils/static/src/css/widgets.css"/>
            <script type="text/javascript" src="/web/static/src/js/owl_compatibility.js"></script>
            <script type="text/javascript" src="/jowebutils/static/src/js/loading.js" />
            <script type="text/javascript" src="/jowebutils/static/dist/module.js" />
        </xpath>
    </template>

</odoo>
